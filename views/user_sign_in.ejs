<div class="container">
    <h1>Sign In</h1>
    <form action="/users/create-session" method="post">
        <input type="email" name="email" placeholder="email_id@xyz.com" required>
        <input type="password" name="password" placeholder="Enter Password" required>
        <input type="submit" value="Sign In">
        <a href="/users/auth/google">Google SignIn/SignUp</a>
    </form>
</div>